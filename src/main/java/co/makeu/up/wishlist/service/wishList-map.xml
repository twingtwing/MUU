<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.makeu.up.wishlist.service.WishListMapper">
	<select id="WishListSearch" resultType="co.makeu.up.wishlist.service.WishlistVO">
		SELECT l.LT_NO,l.THUMB,l.TTL,l.INTRO,l.PRC
		FROM LECTURE l JOIN WISHLIST w
		ON l.lt_no = w.lt_no
		WHERE w.id=#{id}
	</select>
</mapper>